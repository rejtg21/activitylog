    "homepage": "https://github.com/rejtg21/activitylog",
    "keywords":
        [
            "log",
            "user",
            "activity",
            "laravel"
        ],
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be"
        },
        {
            "name" : "Rejev Edward Mediodia",
            "email" : "rejmediodia_21@yahoo.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/rejtg21/activitylog",
            "branches-path" : false,
            "tags-path" : false
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*",
        "activitylog" : "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*"
